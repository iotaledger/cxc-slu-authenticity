{
    "moduleFileExtensions": [
        "js",
        "json",
        "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
        "**/*.ts"
      ],
    "coverageDirectory": "./dist/coverage",
    "testEnvironment": "node",
    "setupFiles": [
        "./jest/envVars.ts"
    ]
}