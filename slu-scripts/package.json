{
	"name": "vpuf",
	"version": "1.0.0",
	"description": "library to encrypt/decrypt data",
	"main": "vpuf.ts",
	"scripts": {
		"format": "prettier --write \"./**/*.ts\" \"./**/tests/**/*.ts\"",
		"build": "tsc",
		"test": "jest",
		"test:cov": "jest --coverage",
		"encrypt-file": "KEY=$npm_config_key INPUT=$npm_config_input DEST=$npm_config_dest node -e \"require('./dist/auth-proof/encrypt-data.js').encryptData();\"",
		"send-proof": "node ./dist/auth-proof/auth-proof.js"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@noble/ed25519": "^1.5.3",
		"axios": "^0.25.0",
		"bs58": "^4.0.1",
		"crypto-js": "^4.1.1"
	},
	"devDependencies": {
		"@types/axios": "^0.14.0",
		"@types/bs58": "^4.0.1",
		"@types/crypto-js": "^4.1.0",
		"@types/jest": "^27.4.0",
		"@types/node": "^17.0.14",
		"ts-jest": "^27.1.3",
		"typescript": "^4.5.5"
	}
}
